leration higher because we want to catch things BEFORE they peak\ncurrent_stats['DaScient_Pulse_Score'] = current_stats['velocity'] + (current_stats['acceleration'] * 2)\n\n# Rank\nranking = current_stats.sort_values('DaScient_Pulse_Score', ascending=False)[['topic', 'y', 'velocity', 'DaScient_Pulse_Score']]\nranking.columns = ['Hashtag', 'Current Views', 'Daily Growth', 'Pulse Score']\n\nprint(\"üèÜ CURRENT VIRAL RANKING (DaScient Algorithm):\")\ndisplay(ranking)\n\n# Visual comparison\nplt.figure(figsize=(12, 6))\nsns.lineplot(data=df, x='ds', y='y', hue='topic')\nplt.title('90-Day Trend History')\nplt.ylabel('Daily Views')\nplt.xlabel('Date')\nplt.grid(True, alpha=0.3)\nplt.show()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-19T00:15:54.787852Z","iopub.execute_input":"2026-01-19T00:15:54.788352Z","iopub.status.idle":"2026-01-19T00:15:55.173607Z","shell.execute_reply.started":"2026-01-19T00:15:54.788318Z","shell.execute_reply":"2026-01-19T00:15:55.172651Z"}},"outputs":[{"name":"stdout","text":"üèÜ CURRENT VIRAL RANKING (DaScient Algorithm):\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"          Hashtag  Current Views  Daily Growth  Pulse Score\n269  #RetroGaming   10333.104513  -1073.368985 -2616.103631","text/html":"<div>\n<style scoped>\n    .dataframe tbody tr th:only-of-type {\n        vertical-align: middle;\n    }\n\n    .dataframe tbody tr th {\n        vertical-align: top;\n    }\n\n    .dataframe thead th {\n        text-align: right;\n    }\n</style>\n<table border=\"1\" class=\"dataframe\">\n  <thead>\n    <tr style=\"text-align: right;\">\n      <th></th>\n      <th>Hashtag</th>\n      <th>Current Views</th>\n      <th>Daily Growth</th>\n      <th>Pulse Score</th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr>\n      <th>269</th>\n      <td>#RetroGaming</td>\n      <td>10333.104513</td>\n      <td>-1073.368985</td>\n      <td>-2616.103631</td>\n    </tr>\n  </tbody>\n</table>\n</div>"},"metadata