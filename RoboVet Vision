from ipywidgets import FileUpload, Output, VBox, Button, Label\nimport io\n\ndef generate_robovet_report(img_content):\n    img = PILImage.create(io.BytesIO(img_content))\n    \n    # 1. Run Breed ID\n    breed, _, probs_b = learn_breed.predict(img)\n    breed_conf = probs_b.max().item()\n    \n    # 2. Run Disease ID (If active)\n    health_status = \"System Offline (Data Missing)\"\n    health_conf = 0.0\n    \n    if learn_disease:\n        cond, _, probs_d = learn_disease.predict(img)\n        health_conf = probs_d.max().item()\n        health_status = cond\n    \n    # 3. Generate \"Hyper-Tech\" Output\n    print(\"\\n\" + \"=\"*40)\n    print(f\"ü§ñ RoboVet‚Ñ¢ DIAGNOSTIC REPORT\")\n    print(\"=\"*40)\n    print(f\"üì∏ Visual Input Analysis Complete.\")\n    print(f\"üê∂ SUBJECT ID:      {breed.replace('_', ' ').upper()}\")\n    print(f\"üìä CONFIDENCE:      {breed_conf*100:.2f}%\")\n    print(\"-\" * 20)\n    print(f\"ü©∫ TRIAGE SCAN:     {health_status}\")\n    print(f\"‚ö†Ô∏è SEVERITY INDEX:  {health_conf*100:.2f}% (Model Certainty)\")\n    \n    # Logic for Recommendation\n    if learn_disease and health_status != 'Healthy' and health_conf > 0.8:\n        print(\"\\nüö® ALERT: Potential pathology detected. Veterinary consultation advised.\")\n    else:\n        print(\"\\n‚úÖ STATUS: No critical anomalies detected by visual scan.\")\n    print(\"=\"*40 + \"\\n\")\n    \n    display(img.to_thumb(256,256))\n\n# UI Widgets\nprint(\"üñ•Ô∏è  Initiating User Interface...\")\nuploader = FileUpload(accept='image/*', multiple=False)\nout = Output()\n\ndef on_upload_change(change):\n    out.clear_output()\n    if not change['new']: return\n    # Get the uploaded file\n    file_info = change['new'][0] # Access the first uploaded file\n    content = file_info['content'] # Access the content bytes\n    \n    with out:\n        generate_robovet_report(content)\n\nuploader.observe(on_upload_change, names='value')\nVBox([Label('üì§ Upload Pet Image for RoboVet Scan:'), uploader, out])","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-19T00:12:39.226082Z","iopub.execute_input":"2026-01-19T00:12:39.226338Z","iopub.status.idle":"2026-01-19T00:12:39.242478Z","shell.execute_reply.started":"2026-01-19T00:12:39.226314Z","shell.execute_reply":"2026-01-19T00:12:39.241639Z"}},"outputs":[{"name":"stdout","text":"üñ•Ô∏è  Initiating User Interface...\n","output_type":"stream"},{"execution_count":3,"output_type":"execute_result","data":{"text/plain":"VBox(children=(Label(value='üì§ Upload Pet Image for RoboVet Scan:'), FileUpload(value=(), accept='image/*', des‚Ä¶","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"2417ea0177b145bc87dcd86e46939f8b"}},"metadata":{}}],"execution_count":3},{"cell_type":"code","source":"# en fin","metadata":{"trusted":true},"outputs":[],"execution_count":null}]}